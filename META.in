names = "deriving-eliom-form"
version = "0.1"
description = "Derive Eliom forms from type declarations"
package "syntax" (
  requires(syntax) = "deriving.syntax"
  archive(preprocessor,syntax) = "pa_deriving_Form.cma"
)
package "syntax_tc" (
  exists_if = "pa_deriving_Form_tc.cma"
  requires(syntax) = "deriving.syntax_tc"
  archive(preprocessor,syntax) = "pa_deriving_Form_tc.cma"
)
package "server" (
  requires = "eliom.server str"
  directory = "server"
  archive(byte) = "@@SERVER_CMO_FILES@@"
)
package "client" (
 requires = "eliom.client str"
 directory = "client"
 archive(byte) = "@@CLIENT_CMO_FILES@@"
)
